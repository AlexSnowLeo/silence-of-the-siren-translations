            <!--Exploration skills-->
            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORER</LocID>                              <Text>Explorador</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORER_DESCRIPTION</LocID>                  <Text><![CDATA[Acrescenta permanentemente <color=SOTS_COLOR_MOVEMENT>4 Pontos de Movimento</color> com cada nível de talento.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXPLORER1</LocID>                           <Text><![CDATA[Acrescenta permanentemente <color=SOTS_COLOR_MOVEMENT>4 Pontos de Movimento.</color>\nReunir-se com qualquer Comandante amigável restaura <color=SOTS_COLOR_MOVEMENT>2 Pontos de Movimento</color> uma vez for Fase.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXPLORER2</LocID>                           <Text><![CDATA[Acrescenta permanentemente <color=SOTS_COLOR_MOVEMENT>8 Pontos de Movimento.</color>\nReunir-se com qualquer Comandante amigável restaura <color=SOTS_COLOR_MOVEMENT>4 Pontos de Movimento</color> uma vez for Fase.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXPLORER3</LocID>                           <Text><![CDATA[Acrescenta permanentemente <color=SOTS_COLOR_MOVEMENT>12 Pontos de Movimento.</color>\nReunir-se com qualquer Comandante amigável restaura <color=SOTS_COLOR_MOVEMENT>6 Pontos de Movimento</color> uma vez for Fase.]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_ENGINEER</LocID>                              <Text>Engenheiro</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_ENGINEER_DESCRIPTION</LocID>                  <Text><![CDATA[Acrescenta uma recarga diária de <color=SOTS_COLOR_ENERGY>5 Pontos de Energia</color> com cada nível de talento, e aprimora outros aspectos de uso de Energia.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_ENGINEER1</LocID>                           <Text><![CDATA[Recarrega <color=SOTS_COLOR_ENERGY>5 Energy Points</color> diariamente.\nEquipamentos drenam <color=SOTS_COLOR_ENERGY>10% menos de Energia.</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_ENGINEER2</LocID>                           <Text><![CDATA[Recarrega <color=SOTS_COLOR_ENERGY>10 Energy Points</color> diariamente.\nArtefatos drenam <color=SOTS_COLOR_ENERGY>10% menos de Energia.</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_ENGINEER3</LocID>                           <Text><![CDATA[Recarrega <color=SOTS_COLOR_ENERGY>15 energy points</color> diariamente.\nAcrescenta <color=SOTS_COLOR_ENERGY>+30 pontos a reserva máxima de Energia</color> do Comandante.]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_NEGOTIATOR</LocID>                            <Text>Negociador</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_NEGOTIATOR_DESCRIPTION</LocID>                <Text>Algumas unidades de cada grupo em um exército neutro se juntarão ao seu Comandante antes da batalha. (Baseado em nível de talento e organização do exército)</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR1</LocID>                         <Text>10%, ou ao menos 1 unidade, de cada grupo irá se juntar.\n(80% do preço original)</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR2</LocID>                         <Text>20%, ou ao menos 1 unidade, de cada grupo irá se juntar.\n(60% do preço original)</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR3</LocID>                         <Text><![CDATA[30%, ou ao menos 1 unidade, de cada grupo irá se juntar.\nRemove <color=SOTS_COLOR_GOLD>Penalidade de Moral</color> para unidades de facções diferentes no exército.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR3_TOOLTIP</LocID>                 <Text>(Negociador no Nível 3 cancelas penalidades para unidades de facções diferentes no exército)</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_BUILDER</LocID>                               <Text>Construtor</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_BUILDER_DESCRIPTION</LocID>                   <Text><![CDATA[Estruturas no Mapa Mundial são construidas na sua <color=SOTS_COLOR_GOLD>forma aprimorada</color>. Nível 3 acrescenta <color=SOTS_COLOR_GOLD>Ciber-armadilhas</color> as estruturas disponíveis.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_CHEAPER_PRODUCTION_UPGRADES</LocID>         <Text><![CDATA[Aprimoramentos de Produção de edifícios no mapa custam <color=SOTS_COLOR_GOLD>{1}%</color> menos.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_CHEAPER_GARRISON_UPGRADES</LocID>           <Text><![CDATA[Aprimoramentos Defensivos de edifícios no mapa são <color=SOTS_COLOR_GOLD>gratuitos!</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_BUILDER_DISCOUNT_PERCENT</LocID>            <Text><![CDATA[Construções de estruturas no mapa recebem <color=SOTS_COLOR_GOLD>{1}%</color> de desconto!]]> </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_TRACKER_DISCOUNT_PERCENT</LocID>            <Text><![CDATA[Remover Xeno-gosmas custa <color=SOTS_COLOR_GOLD>{1}%</color> menos!]]> </Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_SCOUT</LocID>                                 <Text>Patrulheiro</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_SCOUT_DESCRIPTION</LocID>                     <Text><![CDATA[Aumenta o alcançe de visibilidade, fornece mais informações de exércitos inimigos e prevê localizações de tesouros não descobertos. Comandante não é mais afetado por armadilhas e perigos do ambiente.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUT1</LocID>                              <Text><![CDATA[<color=SOTS_COLOR_GOLD>+1 Alcançe de Visibilidade.</color>\n<color=SOTS_COLOR_GOLD>Acrescenta um (?) ícone</color> para tesouros e recursos 5 casas dentro da neblina.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUT2</LocID>                              <Text><![CDATA[<color=SOTS_COLOR_GOLD>+2 Alcançe de Visibilidade.</color>\n<color=SOTS_COLOR_GOLD>Acrescenta um (?) ícone</color> para minas e outras estruturas não-ocupadas 5 casas dentro da neblina.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUT3</LocID>                              <Text><![CDATA[<color=SOTS_COLOR_GOLD>+3 Alcançe de Visibilidade.</color>\n<color=SOTS_COLOR_GOLD>Acrescenta um (?) ícone</color> para equipamentos, artefatos, bases e estruturas benéficas 5 casas dentro da neblina.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SPY1</LocID>                                <Text><![CDATA[Mostra os <color=SOTS_COLOR_GOLD>Atributos</color> do Comandante inimigo.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SPY2</LocID>                                <Text><![CDATA[Mostra os <color=SOTS_COLOR_GOLD>Atributos e Talentos</color> do Comandante inimigo.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SPY3</LocID>                                <Text><![CDATA[Mostra os <color=SOTS_COLOR_GOLD>Atributos e Talentos</color> do Comandante inimigo. Também mostra o tamanho do <color=SOTS_COLOR_GOLD>exército inimigo.</color>]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_COLLECTOR</LocID>                             <Text>Colecionador</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_COLLECTOR_DESCRIPTION</LocID>                 <Text>Acrescenta um espaço extra para artefatos ou equipamentos com cada nível de talento. Permite recolher ítens de locais perigosos próximos a exércitos neutros sem causar uma batalha. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_COLLECTOR1</LocID>                          <Text>+1 espaço de equipamento.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_COLLECTOR2</LocID>                          <Text><![CDATA[+2 espaços de equipamentos.\n +10% de chance ganhar um <color=SOTS_COLOR_FUSION_CORE>Núcleo de Fusão</color> Primal.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_COLLECTOR3</LocID>                          <Text><![CDATA[+1 espaço de artefato.\nJogador pode ver um (?) ícone para <color=SOTS_COLOR_FUSION_CORE>Núcleos de Fusão</color> 5 casas dentro da neblina.]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_TRACKER</LocID>                               <Text>Rastreador</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_TRACKER_DESCRIPTION</LocID>                   <Text><![CDATA[Reduz a penalidade por movimentos em terreno bruto. Mostra trilhas de seus inimigos. O Comandante é ignorado por torretas Primais e outras defesas.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_TERRAIN_COST_REDUCED</LocID>                <Text><![CDATA[<color=SOTS_COLOR_GOLD>Penalidade por terreno bruto</color> reduzida em {1}%.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SEE_ENEMY_PATHS_TILES</LocID>               <Text><![CDATA[Pode ver trilhas de {1} passos de <color=SOTS_COLOR_GOLD>Comandantes inimigos.</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXTRA_MOVE_POINTS_FROM_CONSUMABLES</LocID>  <Text><![CDATA[Consumíveis restauram {1}% a mais de <color=SOTS_COLOR_MOVEMENT>movimento.</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUTING_BONUS</LocID>                      <Text>Alcançe de patrulha +{1} casa(s).</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORATION_ABILITIES</LocID>                 <Text>Viajante</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORATION_ABILITIES_DESCRIPTION</LocID>     <Text><![CDATA[Aprender ou subir o nível de talento Viajante lhe permite aprender uma variedade de <color=SOTS_COLOR_GOLD>Habilidades de Exploração.</color> Após aprender este talento, visite uma base com uma <color=SOTS_COLOR_GOLD>Academia</color> para aprender mais habilidades.]]></Text>  </GameDBLocalizedString>

            <!--Combat skills-->
            <GameDBLocalizedString>         <LocID>HERO_SKILL_BRAWLER</LocID>                               <Text>Porradeiro</Text>  </GameDBLocalizedString>
