            <GameDBLocalizedString>         <LocID>MAPOBJ_GOLD_MINE</LocID>                                           <Text>Operação Moedeira</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>MAPOBJ_GOLD_MINE_DESCRIPTION</LocID>                               <Text><![CDATA[Produz <color=SOTS_COLOR_GOLD>+500</color> (<color=SOTS_COLOR_GOLD>+1000</color> se aprimorada) <color=SOTS_COLOR_GOLD>Aura Coins</color> cada turno.]]></Text>  </GameDBLocalizedString>
                                                                                                                      
            <GameDBLocalizedString>         <LocID>MAPOBJ_WATCHTOWER_01</LocID>                                       <Text>Torre de Vigia</Text>  </GameDBLocalizedString>
